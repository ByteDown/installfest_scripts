[user]
  name = github_name
  email = github_email
[color]
  ui = always
[alias]
  st = status -sb -uall
  lg = log --decorate --pretty=oneline --abbrev-commit --graph
  undocommit = reset --soft HEAD^
  undopush = push -f origin HEAD^:master
[core]
  editor = subl
  excludesfile = ~/.gitignore_global
[commit]
  template = ~/.gitmessage.txt
[branch]
  autosetuprebase = always
